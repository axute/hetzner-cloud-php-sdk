    "hcloud",
    "hetzner cloud",
    "cloud php library"
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/collections": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas KÃ¤mmerling",
      "email": "kontakt@lukas-kaemmerling.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "LKDev\\HetznerCloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LKDev\\Tests\\": "tests/"
    }
